{
  "name": "nodeJS",
  "version": "1.0.0",
  "description": "demo project",
  "main": "index.js",
  "author": "Ruslana Hural",
  "license": "MIT",
  "scripts": {
    "start": "rimraf ./dist && babel -d ./dist ./src -s && node dist/main.js",
