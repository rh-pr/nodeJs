  "pre-commit": "lint-staged",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2"
